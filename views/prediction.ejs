    <script>
    //graph1 for top countries by rounds
        angular.module('myModule', [])
        .controller('myController', function($scope,$http) {
            $http.get("/getData")
            .then(function(response) {
                Highcharts.chart('container', {
                  chart: {
                      type: 'column'
                  },
                  title: {
                      text: 'FitBit Heart Rate Zones'
                  },
                  subtitle: {
                      text: 'Source: <a href="http://fitbit.com">FitBit</a>'
                  },
                  xAxis: {
                      type: 'category',
                      labels: {
                          rotation: -45,
                          style: {
                              fontSize: '13px',
                              fontFamily: 'Verdana, sans-serif'
                          }
                      }
                  },
                  yAxis: {
                      min: 0,
                      title: {
                          text: 'Heart Rate'
                      }
                  },
                  legend: {
                      enabled: false
                  },
                  tooltip: {
                      pointFormat: 'Heart Rate <b>{point.y:.1f} millions</b>'
                  },
                  series: [{
                      name: 'Population',
                      data: response.data,
                      dataLabels: {
                          enabled: true,
                          rotation: -90,
                          color: '#FFFFFF',
                          align: 'right',
                          format: '{point.y:.1f}', // one decimal
                          y: 10, // 10 pixels down from the top
                          style: {
                              fontSize: '13px',
                              fontFamily: 'Verdana, sans-serif'
                          }
                        }
                    }]
                });
           });

            //graph2 for top regions by investments
            $http.get("/getRegions")
            .then(function(response) {
                console.log(response.data.dates);
                console.log(response.data.monthData);
                Highcharts.chart('container2', {
                chart: {
                    type: 'line'
                },
                title: {
                    text: 'December Month Heart Rate'
                },
                subtitle: {
                    text: 'Source: Fitbit.com'
                },
                xAxis: {
                    categories: response.data.dates
                },
                yAxis: {
                    title: {
                        text: 'Heart rate'
                    }
                },
                plotOptions: {
                    line: {
                        dataLabels: {
                            enabled: true
                        },
                        enableMouseTracking: false
                    }
                },
                series: [{
                    name: 'Ruchit',
                    data: response.data.monthData
                }]
             });
            });
        });
    </script>

